pipeline {
    
    agent {label 'C-slave'}
    
	stages {
		stage ('clone') {
			steps {
			git branch: 'main', credentialsId: 'githubprivate', url: 'https://github.com/Rakeshgoniguntla/assignment.git'
				}
			}
		stage ('Build') {
			steps {
				sh '''
				cd /home/ubuntu/jenkins/workspace/Archive
				make 
				'''
				}
				post {
					success {
					
						archiveArtifacts artifacts: '*.exe', followSymlinks: false
						}
					}
				
				}
			
			}
		}
